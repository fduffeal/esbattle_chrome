{
    "manifest_version": 2,
    "name": "EsBattle Apps",
    "description": "chrome application for www.esbattle.com",
    "version": "1.1",
    "icons": {
        "128": "128.png"
    },
    "permissions": [
        "unlimitedStorage",
        "notifications",
        "http://www.esbattle.com/",
        "http://app.esbattle.com/",
        "https://secure.flickr.com/"
    ],
    "browser_action": {
        "default_icon": "128.png",
        "default_popup": "index.html"
    }
}